<%- include('./partials/headerPanel') %>

    <div class="container">
        <% if (user.length> 0) {%>
            <table class="table table-striped table-bordered" id="productosTable">
                <thead>
                    <tr>
                        <th>Nombres</th>
                        <th>Apellidos</th>
                        <th>Email</th>
                        <th>Rol</th>
                        <th>Acciones</th>
                    </tr>
                </thead>
                <tbody>
                    <% user.forEach(item=> { %>
                        <tr>
                            <td>
                                <%=item.firstName %>
                            </td>
                            <td>
                                <%=item.lastName %>
                                    <%=item.motherLastName %>
                            </td>
                            <td>
                                <%=item.email %>
                            </td>
                            <td>
                                <%=item.nameRole %>
                            </td>
                            <th></th>
                        </tr>
                        <% }) %>



                </tbody>
            </table>


            <%}else{%>
                <span>Pedidos no disponibles</span>

                <% } %>

                    <%- include('./partials/footerPanel') %>